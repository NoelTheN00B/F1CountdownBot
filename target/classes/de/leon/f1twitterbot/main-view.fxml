<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextField?>
<VBox alignment="CENTER" spacing="20.0" xmlns:fx="http://javafx.com/fxml"
  fx:controller="de.leon.f1twitterbot.MainController">
  <padding>
    <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
  </padding>

  <Button text="Set Config" onAction="#setConfigButtonClick"/>
  <Label fx:id="apiKeyLabel" text="API Key"/>
  <TextField fx:id="apiKeyTextfield"/>
  <Label fx:id="apiSecretKeyLabel" text="API Key"/>
  <TextField fx:id="apiSecretKeyTextfield"/>
  <Label fx:id="accessTokenLabel" text="API Key"/>
  <TextField fx:id="accessTokenTextfield"/>
  <Label fx:id="accessTokenSecretLabel" text="API Key"/>
  <TextField fx:id="accessTokenSecretTextfield"/>
</VBox>
